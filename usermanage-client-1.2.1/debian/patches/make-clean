+                        'm4_pattern_allow' => 1,
+                        'AM_AUX_DIR_EXPAND' => 1,
+                        'm4_pattern_forbid' => 1,
+                        'AM_MISSING_PROG' => 1,
+                        'AM_DEP_TRACK' => 1,
+                        'AM_PROG_CC_C_O' => 1,
+                        'AM_AUTOMAKE_VERSION' => 1,
+                        'AM_MISSING_HAS_RUN' => 1,
+                        '_AM_SUBST_NOTMAKE' => 1,
+                        'AM_SET_DEPDIR' => 1,
+                        '_AC_AM_CONFIG_HEADER_HOOK' => 1,
+                        '_AM_OUTPUT_DEPENDENCY_COMMANDS' => 1,
+                        '_AM_MANGLE_OPTION' => 1,
+                        '_AM_IF_OPTION' => 1,
+                        'AM_PROG_INSTALL_SH' => 1,
+                        '_AM_CONFIG_MACRO_DIRS' => 1,
+                        'AM_SUBST_NOTMAKE' => 1,
+                        'AU_DEFUN' => 1
+                      }
+                    ], 'Autom4te::Request' ),
+             bless( [
+                      '1',
+                      1,
+                      [
+                        '/usr/share/autoconf'
+                      ],
+                      [
+                        '/usr/share/autoconf/autoconf/autoconf.m4f',
+                        'aclocal.m4',
+                        'configure.ac'
+                      ],
+                      {
+                        'AC_INIT' => 1,
+                        'AC_CANONICAL_SYSTEM' => 1,
+                        'AM_AUTOMAKE_VERSION' => 1,
+                        'AH_OUTPUT' => 1,
+                        'AM_GNU_GETTEXT' => 1,
+                        'm4_pattern_forbid' => 1,
+                        'AM_PROG_CC_C_O' => 1,
+                        'AC_FC_PP_DEFINE' => 1,
+                        '_AM_COND_ELSE' => 1,
+                        'AM_EXTRA_RECURSIVE_TARGETS' => 1,
+                        'AC_PROG_LIBTOOL' => 1,
+                        'sinclude' => 1,
+                        'AM_MAINTAINER_MODE' => 1,
+                        'AC_REQUIRE_AUX_FILE' => 1,
+                        'AM_PROG_CXX_C_O' => 1,
+                        'AM_PROG_FC_C_O' => 1,
+                        'AM_MAKEFILE_INCLUDE' => 1,
+                        'm4_sinclude' => 1,
+                        'LT_CONFIG_LTDL_DIR' => 1,
+                        'AM_GNU_GETTEXT_INTL_SUBDIR' => 1,
+                        'AC_FC_FREEFORM' => 1,
+                        'AM_PATH_GUILE' => 1,
+                        'AM_NLS' => 1,
+                        'AM_PROG_MOC' => 1,
+                        '_AM_MAKEFILE_INCLUDE' => 1,
+                        '_AM_SUBST_NOTMAKE' => 1,
+                        'AM_CONDITIONAL' => 1,
