+                        'AM_OUTPUT_DEPENDENCY_COMMANDS' => 1,
+                        '_AM_AUTOCONF_VERSION' => 1,
+                        'AM_RUN_LOG' => 1,
+                        'AM_CONDITIONAL' => 1,
+                        '_AM_PROG_TAR' => 1,
+                        'GTK_CHECK_BACKEND' => 1,
+                        'AM_PROG_INSTALL_STRIP' => 1,
+                        'include' => 1,
+                        '_AM_SET_OPTIONS' => 1,
+                        '_m4_warn' => 1,
+                        'AM_MAKE_INCLUDE' => 1,
+                        'AM_SILENT_RULES' => 1,
+                        'AC_DEFUN' => 1,
