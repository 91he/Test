+                        'AM_SET_CURRENT_AUTOMAKE_VERSION' => 1,
+                        'AM_SANITY_CHECK' => 1,
+                        '_AM_PROG_CC_C_O' => 1,
+                        'AC_DEFUN_ONCE' => 1,
